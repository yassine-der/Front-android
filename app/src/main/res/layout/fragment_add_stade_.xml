<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragement.add_stade_Fragment">

    <com.mapbox.maps.MapView
        android:id="@+id/mapView1"
        android:layout_width="392dp"
        android:layout_height="650dp"
        android:layout_marginBottom="59dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        mapbox:layout_constraintBottom_toBottomOf="parent"
        mapbox:layout_constraintHorizontal_bias="0.473"
        mapbox:mapbox_cameraTargetLat="36.89985"
        mapbox:mapbox_cameraTargetLng="10.189562"
        mapbox:mapbox_cameraZoom="5.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab_style_toggle1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="16dp"
                android:layout_marginBottom="80dp"
                android:contentDescription="TODO"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                mapbox:layout_constraintBottom_toBottomOf="parent"
                mapbox:layout_constraintHorizontal_bias="0.982"
                mapbox:srcCompat="@drawable/ic_baseline_map_24"

                tools:backgroundTint="@color/green"
                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.mapbox.maps.MapView>


</androidx.constraintlayout.widget.ConstraintLayout>

